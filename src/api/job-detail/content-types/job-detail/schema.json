{
  "kind": "collectionType",
  "collectionName": "job_details",
  "info": {
    "singularName": "job-detail",
    "pluralName": "job-details",
    "displayName": "Job Detail"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "job_title": {
      "type": "string"
    },
    "text_icon": {
      "type": "json"
    },
    "salary_range": {
      "type": "string"
    },
    "question_1": {
      "type": "text"
    },
    "question_2": {
      "type": "text"
    },
    "question_3": {
      "type": "text"
    },
    "job_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "job": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::job.job"
    }
  }
}
