{
  "kind": "collectionType",
  "collectionName": "job_details",
  "info": {
    "singularName": "job-detail",
    "pluralName": "job-details",
    "displayName": "Job Detail"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "job_title": {
      "type": "string"
    },
    "salary_range": {
      "type": "string"
    },
    "overview": {
      "type": "text"
    },
    "requirements": {
      "type": "text"
    },
    "benefits": {
      "type": "text"
    },
    "job_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "job": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::job.job"
    },
    "years_experience": {
      "type": "integer"
    },
    "level": {
      "type": "string"
    }
  }
}
